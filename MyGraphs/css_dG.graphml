      <data key="d2">1</data>
      <data key="d3">magenta</data>
    </edge>
    <edge source="1" target="3">
      <data key="d2">3</data>
      <data key="d3">yellow</data>
    </edge>
    <edge source="3" target="2">
      <data key="d2">5</data>
